<%= include ../myHeader.ejs %>
      <a href="/signup" class="btn btn-default btn-lg">Sign Up</a>
      <a href="/login" class="btn btn-default btn-lg">Log In</a>

      <img src="images/logo.jpg" class="logo"/>

      <h2>All Posts</h2>
      <ul>
      <% posts.forEach(function(post){ %>
          <li><h3><a href="/posts/<%= post.dataValues.id %>"><%= post.dataValues.title%></a></h3> <p><%= post.dataValues.body%></p></li>
        <% }) %>
      </ul> 
      
<%= include ../myFooter.ejs %>